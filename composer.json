{
    "name": "iutrace/laravel-support",
    "description": "Laravel support helpers, contracts, and traits. Validator functionality, and basic controller included out-of-the-box. Forked from rinvex/laravel-support.",
    "type": "library",
    "keywords": [
        "iutrace",
        "laravel",
        "contract",
        "validator",
        "timezones",
        "mimetype",
        "support",
        "helper",
        "trait"
    ],
    "license": "MIT",
    "homepage": "https://github.com/iutrace/laravel-support",
    "support": {
        "issues": "https://github.com/iutrace/laravel-support/issues",
        "source": "https://github.com/iutrace/laravel-support",
        "docs": "https://github.com/iutrace/laravel-support/blob/main/README.md"
    },
    "authors": [
        {
            "name": "iutrace",
            "homepage": "https://github.com/iutrace"
        },
        {
            "name": "Original Authors - Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com",
            "role": "Original Package Authors"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/iutrace/laravel-support/contributors"
        }
    ],
    "require": {
        "php": "^8.3.0",
        "illuminate/database": "^11.0.0 || ^12.0.0",
        "illuminate/support": "^11.0.0 || ^12.0.0",
        "spatie/laravel-sluggable": "^3.6.0",
        "spatie/laravel-schemaless-attributes": "^2.5.0",
        "spatie/laravel-translatable": "^6.5.0",
        "vinkla/hashids": "^12.0.0 || ^13.0.0",
        "watson/validating": "^8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Iutrace\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iutrace\\Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iutrace\\Support\\Providers\\SupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
